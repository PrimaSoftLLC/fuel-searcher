<?xml version='1.0' encoding='UTF-8' ?>
<fuel-tables>

    <simple-fuel-tables>

        <!-- TABLE #1 -->
        <fuel-table>

            <name>ВСПАШКА ПЛАСТА МНОГОЛЕТНИХ ТРАВ</name>

            <fuel-headers>
                <fuel-header>Менее 150</fuel-header>
                <fuel-header>150-200</fuel-header>
                <fuel-header>201-300</fuel-header>
                <fuel-header>301-400</fuel-header>
                <fuel-header>401-600</fuel-header>
                <fuel-header>601-1000</fuel-header>
                <fuel-header>Более 1000</fuel-header>
            </fuel-headers>

            <filter-chain>
                <filter-by-group>удельное сопротивление</filter-by-group>
                <filter-by cell-index="1">трактор</filter-by>
                <filter-by cell-index="2">механизм</filter-by>
                <filter-by cell-index="3">количество корпусов</filter-by>
                <final-filter-by cell-index="4">глубина вспашки</final-filter-by>
            </filter-chain>

            <fuel-header-cell-property>длина гона</fuel-header-cell-property>

        </fuel-table>

        <!-- TABLE #2 -->
        <fuel-table>

            <name>ВСПАШКА СТЕРНИ</name>

            <fuel-headers>
                <fuel-header>Менее 150</fuel-header>
                <fuel-header>150-200</fuel-header>
                <fuel-header>201-300</fuel-header>
                <fuel-header>301-400</fuel-header>
                <fuel-header>401-600</fuel-header>
                <fuel-header>601-1000</fuel-header>
                <fuel-header>Более 1000</fuel-header>
            </fuel-headers>

            <filter-chain>
                <filter-by-group>удельное сопротивление</filter-by-group>
                <filter-by cell-index="1">трактор</filter-by>
                <filter-by cell-index="2">механизм</filter-by>
                <filter-by cell-index="3">количество корпусов</filter-by>
                <final-filter-by cell-index="4">глубина вспашки</final-filter-by>
            </filter-chain>

            <fuel-header-cell-property>длина гона</fuel-header-cell-property>

        </fuel-table>

    </simple-fuel-tables>

</fuel-tables>
