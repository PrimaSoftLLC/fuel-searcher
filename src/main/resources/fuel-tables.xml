<?xml version='1.0' encoding='UTF-8' ?>
<fuel-tables>

    <simple-fuel-tables>

        <fuel-table>

            <name>ВСПАШКА ПЛАСТА МНОГОЛЕТНИХ ТРАВ</name>

            <fuel-info-headers>
                <fuel-info-header>Менее 150</fuel-info-header>
                <fuel-info-header>150-200</fuel-info-header>
                <fuel-info-header>201-300</fuel-info-header>
                <fuel-info-header>301-400</fuel-info-header>
                <fuel-info-header>401-600</fuel-info-header>
                <fuel-info-header>601-1000</fuel-info-header>
                <fuel-info-header>Более 1000</fuel-info-header>
            </fuel-info-headers>

            <filter-chain>

                <intermidiate-filters>

                    <group-filter>
                        <filter-by>удельное сопротивление</filter-by>
                    </group-filter>

                    <filter>
                        <filter-by>трактор</filter-by>
                        <property-cell-index>1</property-cell-index>
                    </filter>

                    <filter>
                        <filter-by>механизм</filter-by>
                        <property-cell-index>2</property-cell-index>
                    </filter>

                    <filter>
                        <filter-by>количество корпусов</filter-by>
                        <property-cell-index>3</property-cell-index>
                    </filter>

                </intermidiate-filters>

                <conclusive-filter>
                    <filter-by>глубина вспашки</filter-by>
                    <property-cell-index>4</property-cell-index>
                </conclusive-filter>

            </filter-chain>

        </fuel-table>

    </simple-fuel-tables>

</fuel-tables>
